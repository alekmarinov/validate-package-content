{
	"*": {
		"files": [
			{
				"pattern": "manifest.json",
				"optional": true
			},
			{
				"pattern": ".*\\.md5",
				"optional": true
			}
		]
	},
	"audio_book": {
		"package": "(db\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": ".*\\.dtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.pdtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.wav",
				"optional": true
			},
			{
				"pattern": ".*\\.als",
				"optional": true
			}
		]
	},
	"audio_magazine": {
		"package": "(.*?_\\d{4}-\\d{2}).*?\\.pkg\\.zip",
		"files": [
			{
				"pattern": ".*?\\.dtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*?\\.pdtb\\.zip",
				"optional": false
			}
		]
	},
	"audio_music": {
		"package": "(d[br]m\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": ".*\\.dtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.pdtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.wav",
				"optional": true
			},
			{
				"pattern": ".*\\.als",
				"optional": true
			}
		]
	},
	"audio_foreign": {
		"package": "(db[ef]\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": ".*\\.dtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.pdtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.wav",
				"optional": true
			},
			{
				"pattern": ".*\\.als",
				"optional": true
			}
		]
	},
	"braille_book": {
		"package": "br\\.(\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": "v\\d+\\.brf",
				"optional": false
			},
			{
				"pattern": ".*\\.bopf",
				"optional": false
			}
		]
	},
	"braille_magazine": {
		"package": "br\\.(\\w+)_\\d{4}-\\d{2}(-\\d{2})?\\.zip",
		"files": [
			{
				"pattern": "\\w+\\d{2}(\\w{1})?\\.brf",
				"optional": false
			},
			{
				"pattern": "\\w+\\d{2}\\.bopf",
				"optional": false,
				"count": 1
			}
		]
	},
	"braille_music": {
		"package": "(brm\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": "m\\d+\\.brf",
				"optional": false
			},
			{
				"pattern": ".*\\.bopf",
				"optional": false
			}
		]
	},
	"marrakesh": {
		"package": "(\\w+\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": ".*\\.dtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.pdtb\\.zip",
				"optional": false
			},
			{
				"pattern": "\\.zip",
				"optional": true
			}
		]
	},
	"network_audio_book": {
		"package": "(\\w+\\d+)\\.pkg\\.zip",
		"files": [
			{
				"pattern": ".*\\.dtb\\.zip",
				"optional": false
			},
			{
				"pattern": ".*\\.pdtb\\.zip",
				"optional": false
			}
		]
	},
		"non_nls_braille": {
			"package": "(brc\\w+)\\.pkg\\.zip",
			"files": [
				{
					"pattern": "c\\d+.brf",
					"optional": false
				},
				{
					"pattern": ".*\\.bopf",
					"optional": false
				}
			]
	}
}
